{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "space-before-function-paren": 0,
    "linebreak-style": 0,
    "global-require": 0,
    "eol-last": 0,
    "comma-dangle": 0,
    "spaced-comment": 0
  }
}
